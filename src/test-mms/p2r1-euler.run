# MASA p=2, ref 1 runfile

# Mesh
MESH periodic-cube.mesh
REF_LEVELS 1

# output dir
OUTPUT_NAME p2r1-euler-output

# Basis and quadrature
POL_ORDER 2
BASIS_TYPE 0
INT_RULE 0

# Time scheme, time step and number of steps
TIME_INTEGRATOR 4
DT_FIXED 1.953125e-05
NMAX 512
ITERS_OUT 2

# Inviscid flux (Roe or Lax)
USE_ROE 0

# Euler eqns
EQ_SYSTEM 0

# Fluid parameters
FLUID 0
VISC_MULT 0
